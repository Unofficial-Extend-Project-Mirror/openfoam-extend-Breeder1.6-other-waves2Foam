FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      environmentalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

seaLevel	5.000;
seaLevelAsReference true;

relaxationNames ();

initializationName cyclic1;

cyclic1Coeffs
{
    // Solitary first can be applied, but less accurate
    // waveType	solitaryFirst;
    // height	2.0;
    // depth       5;
    // direction   (1 0 0);
    // x0          (150 0 0);

    waveType	chappelear1962;
    height	2.0;
    depth       5;
    direction   (1 0 0);
    x0          (150 0 0);
};

