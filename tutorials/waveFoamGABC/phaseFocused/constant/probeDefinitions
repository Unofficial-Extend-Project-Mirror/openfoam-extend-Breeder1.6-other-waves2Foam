FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      loggingProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

surfaceElevation
{
    type waveGauge;

    pointDistribution lineDistribution;
    N 414;
    stretch   1;
    linestart (-12.19875 0 -.45);
    lineend   (-0.00375 0 -.45);
    add       ( 0 0 .8);
    axis      z;   

    writeControl adjustableRunTime;
    writeInterval 0.025;
}


