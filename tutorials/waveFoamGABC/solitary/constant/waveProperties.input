FoamFile
{
    version	2.0;
    format	ascii;
    class	dictionary;
    object	waveProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

seaLevel            0.00125;
seaLevelAsReference true;

relaxationNames     ();

initializationName  inlet;

inletCoeffs
{
    waveType    chappelear1962;

    x0          (-10 0 0);
    
    height      0.15;
    depth       0.50125;
    direction   (1 0 0);

    GABCSettings
    {
        preProcessMethod   constantDepth;
        celerityFactor     1.11;
    }
}


outletCoeffs
{
    waveType potentialCurrent;
    U        (0 0 0);
    Tsoft    2.0;

    GABCSettings
    {
        preProcessMethod   constantDepth;
        celerityFactor     1.11;
        depth              0.50125;
    }
}


